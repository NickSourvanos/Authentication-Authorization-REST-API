{
    "title": "System user description",
    "properties": {
      "email": {
        "type": "string",
        "pattern": ".+@.+"
      },
      "name": {
        "type": "string",
        "pattern": "[a-z0-9]+"
      },
      "id": {
        "type": "number"
      },
      "password": {
        "type": "string",
        "pattern": "[a-z0-9]+"
      },
      "role": {
        "type": "string",
        "enum": ["su", "admin", "user"]
      },
      "social": {
        "type": "boolean"
      },
      "application": {
        "type": "string",
        "pattern": "[a-z0-9]+"
      },
      "active": {
        "type": "boolean"
      }
    },
    "type": "object"
  }